<script>

function toggleVisibility(ids) {
    ids.forEach((id) => {
        var el = document.getElementById(id);
        if (el.classList.contains('is-mobile-pay--hidden')) {
            el.classList.remove('is-mobile-pay--hidden');
        } else {
            el.classList.add('is-mobile-pay--hidden');
        }
    })
}

function onClick() {
    toggleVisibility(['js-mobile-pay-button', 'js-mobile-pay-cover', 'js-right-column'])
}

</script>


<div id="js-mobile-pay-button" class="buy-button-container top-shadow is-hidden-desktop is-hidden-tablet">
