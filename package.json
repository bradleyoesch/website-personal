{
  "name": "website-personal",
  "version": "2.0.0",
  "description": "App to generate static files to serve at http://bradleyoesch.com",
  "homepage": "https://github.com/bradleyoesch/website-personal#readme",
  "bugs": {
    "url": "https://github.com/bradleyoesch/website-personal/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradleyoesch/website-personal.git"
  },
  "license": "MIT",
  "author": "Bradley Oesch",
  "type": "module",
  "scripts": {
    "build": "radish build",
    "clean": "rm -rf build/",
    "clean-all": "rm -rf node_modules/ && npm run clean",
    "lint": "npx eslint --max-warnings=0 --ext .tsx,.ts,.js,.jsx src/ --fix",
    "lint-all": "npm run lint & npm run lint-styles",
    "lint-styles": "stylelint --fix \"src/{scss,styles}/**/*.{css,scss}\"",
    "prepare": "husky install",
    "start": "radish dev",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*": "npm run lint-all"
  },
  "dependencies": {
    "mustache": "^4.1.0",
    "pre-commit": "^1.2.2",
    "radish": "^0.1.22",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "save": "^2.4.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-radish": "^0.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "sass": "^1.32.8",
    "typescript": "^4.6.3"
  }
}
